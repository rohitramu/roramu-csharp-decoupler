    "version": "2.0.0",
    "configurations": [
        {
            "name": "Test app - .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/test/RoRamu.Decoupler.DotNet.Generator.Test/bin/Debug/net5.0/RoRamu.Decoupler.DotNet.Generator.Test.dll",
            "args": [],
            "cwd": "${workspaceFolder}/test/RoRamu.Decoupler.DotNet.Generator.Test",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Generator.Transmitter.App - .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/src/RoRamu.Decoupler.DotNet.Generator.Transmitter.App/bin/Debug/net5.0/RoRamu.Decoupler.DotNet.Generator.Transmitter.App.exe",
            "args": [
                "${workspaceFolder}/test/RoRamu.Decoupler.DotNet.Generator.Test/bin/Debug/net5.0/RoRamu.Decoupler.DotNet.Generator.Test.dll",
                "RoRamu.Custom.Namespace"
            ],
            "cwd": "${workspaceFolder}",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}